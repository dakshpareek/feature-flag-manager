{
  "env": {
    "node": true,
    "jest": true
  },
  "root" : true,
  "extends": ["airbnb-base", "plugin:security/recommended"],
  "plugins": ["security"],
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "rules": {
    "no-console": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "security/detect-object-injection": "off"
  }
}
